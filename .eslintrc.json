{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {},
    "overrides": [
        {
            "files": ["*.ts"],
            "rules": {
                "array-bracket-spacing": "off",
                "brace-style": "off",
                "block-spacing": "off",
                "indent": "off",
                "max-len": "off",
                "object-curly-spacing": "off",
                "require-jsdoc": "off"
            }
        }
    ]
}